{
	"name": "github-search-vue",
	"version": "1.0.0",
	"description": "Small app to search GitHub repositories",
	"scripts": {
		"webpack:dev": "webpack --mode development --watch",
		"webpack:prod": "webpack --mode production",
		"server:dev": "pm2-dev start server.js",
		"server:prod": "pm2 start server.js --name 'github-search-vue'",
		"server:docker": "pm2-runtime start server.js --name 'github-search-vue'",
		"server-stop:prod": "pm2 stop github-search-vue",
		"preprod": "npm run webpack:prod",
		"prod": "npm run server:prod",
		"prod-stop": "npm run server-stop:prod",
		"prestart:docker": "npm run webpack:prod",
		"start:docker": "npm run server:docker",
		"start": "npm run webpack:dev & npm run server:dev",
		"test": "npm run type-check && npm run lint && npm run prettier && jest",
		"type-check": "tsc",
		"jest": "jest --watch",
		"lint": "eslint 'src/**/*.{ts,tsx}'",
		"prettier": "prettier --check 'src/**/*.{ts,tsx}'"
	},
	"dependencies": {
		"@vue/composition-api": "^0.6.7",
		"express": "^4.17.1",
		"pm2": "^4.4.0",
		"vue": "^2.6.11"
	},
	"devDependencies": {
		"@babel/core": "^7.10.3",
		"@babel/plugin-transform-runtime": "^7.10.3",
		"@babel/preset-env": "^7.10.3",
		"@babel/preset-typescript": "^7.10.1",
		"@babel/register": "^7.10.3",
		"@babel/runtime": "^7.10.3",
		"@types/jest": "^26.0.3",
		"@types/node": "^14.0.14",
		"@typescript-eslint/eslint-plugin": "^3.4.0",
		"@typescript-eslint/parser": "^3.4.0",
		"@vue/compiler-sfc": "^3.0.0-beta.15",
		"css-loader": "^3.6.0",
		"eslint": "^7.3.1",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-vue": "^6.2.2",
		"html-webpack-plugin": "^4.3.0",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^26.1.0",
		"mini-css-extract-plugin": "^0.9.0",
		"nib": "^1.1.2",
		"optimize-css-assets-webpack-plugin": "^5.0.3",
		"prettier": "^2.0.5",
		"style-loader": "^1.2.1",
		"stylus": "^0.54.7",
		"stylus-loader": "^3.0.2",
		"terser-webpack-plugin": "^3.0.6",
		"ts-loader": "^7.0.5",
		"typescript": "^3.9.5",
		"vue-eslint-parser": "^7.1.0",
		"vue-loader": "^15.9.3",
		"vue-template-compiler": "^2.6.11",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.12"
	},
	"main": "server.js",
	"author": "Alvaro Garcia",
	"license": "MIT"
}
